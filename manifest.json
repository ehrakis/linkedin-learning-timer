{
    "manifest_version": 2,
    "name": "Linkedin chapter time",
    "description": "This extension display the time of linkedin videos chapter.",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["https://www.linkedin.com/learning/*"],
        "all_frames": true,
        "js":      ["jquery-3.5.1.min.js","content.js"]
    }]
}